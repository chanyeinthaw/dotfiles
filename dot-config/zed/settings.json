  "chat_panel": {
    "dock": "left"
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "language_models": {
    "openai": {}
  },
  "autosave": "on_focus_change",
  "auto_update": true,
  "relative_line_numbers": true,
  "tab_bar": {
    "show_nav_history_buttons": false
  },
  "tabs": {
    "git_status": true
  },
  "base_keymap": "VSCode",
  "buffer_font_family": "MesloLGL Nerd Font",
  "buffer_font_size": 13,
  "cursor_blink": false,
  "terminal": {
    "font_family": "MesloLGL Nerd Font",
    "font_size": 13
  },
  "vim_mode": true,
  "vim": {
    "use_system_clipboard": "always"
  },
  "scrollbar": {
    "show": "never"
  },
  "project_panel": {
    "dock": "right",
    "git_status": true,
    "indent_size": 25,
    "sticky_scroll": false
  },
  "outline_panel": {
    "dock": "right"
  },
  "git": {
    "git_gutter": "tracked_files"
  },
  "formatter": "auto",
  "format_on_save": "on",
  "private_files": [
    "**/*.pem",
    "**/*.key",
    "**/*.cert",
    "**/*.crt",
    "**/secrets.yml"
  ],
  "remove_trailing_whitespace_on_save": false,
  "lsp": {
    "vtsls": {
      "settings": {
        "typescript": {
          "preferences": {
            "importModuleSpecifier": "non-relative"
          }
        }
      }
    },
    "biome": {
      "settings": {
        "require_config_file": true
      }
    }
  },
  "languages": {
    "TypeScript": {
      "language_servers": ["vtsls"]
    }
  }
}
